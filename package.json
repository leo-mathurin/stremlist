{
  "name": "stremlist",
  "version": "1.0.0",
  "description": "Stremlist - Sync your IMDb watchlist with Stremio",
  "main": "addon.js",
  "scripts": {
    "start": "node addon.js",
    "dev": "nodemon addon.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:redis": "docker-compose -f docker-compose.dev.yml up -d redis",
    "docker:redis:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:all": "docker-compose up -d",
    "docker:all:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "dev:docker": "dev.bat",
    "dev:docker:rebuild": "dev.bat --rebuild",
    "dev:docker:clean": "dev.bat --clean --rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leo-mathurin/imdb-watchlist-stremio.git"
  },
  "keywords": [
    "stremio",
    "addon",
    "imdb",
    "watchlist",
    "stremlist"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leo-mathurin/imdb-watchlist-stremio/issues"
  },
  "homepage": "https://github.com/leo-mathurin/imdb-watchlist-stremio#readme",
  "dependencies": {
    "axios": "^1.6.8",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "ioredis": "^5.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
