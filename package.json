{
  "name": "imdb-watchlist-stremio",
  "version": "1.0.0",
  "description": "IMDb Watchlist for Stremio - Browse your IMDb watchlist in Stremio",
  "main": "addon.js",
  "scripts": {
    "start": "node addon.js",
    "dev": "nodemon addon.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:redis": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:redis:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:all": "docker-compose up -d",
    "docker:all:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leo-mathurin/imdb-watchlist-stremio.git"
